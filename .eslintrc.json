{
    "extends": "react-app",
    "plugins": ["prettier"],
    "rules": {
    "prettier/prettier": "error",
    "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "hrefLeft", "hrefRight", "to" ],
            "aspects": [ "noHref", "invalidHref", "preferButton" ]
        }]
    }
}